<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN'
  'http://mybatis.org/dtd/mybatis-3-mapper.dtd'>

<mapper namespace='modelo.dao.mappers.ValoracionMapper'>


		<insert id='insertValoracion' parameterType='ValorcionPojo'
		useGeneratedKeys='true' keyProperty='id'>
		insert into valoracion
		(idProducto,emailUsuario,valoraciones,comentarios)
		VALUES(#{idProducto}, #{emailUsuario}, 
		#{valoraciones},#{comentarios})
	</insert>



</mapper>